{
  "expo": {
    "name": "Luftio",
    "slug": "luftio-app",
    "owner": "luftio",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "primaryColor": "#031846",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/android-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "androidStatusBar": {
      "translucent": true
    },
    "ios": {
      "supportsTablet": false,
      "icon": "./assets/ios-icon.png"
    },
    "web": {
      "barStyle": "default"
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#031846"
    },
    "description": "Luftio mobile app for Business customers.",
    "userInterfaceStyle": "light"
  }
}
